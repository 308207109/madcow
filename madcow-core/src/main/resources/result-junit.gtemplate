<?xml version="1.0" encoding="UTF-8"?>
<testsuite errors="$errorCount" failures="$failureCount" hostname="$hostname" name="$testName" tests="1" time="$time" timestamp="$timestamp">
    <properties/>
    <testcase classname="$testName" name="$testName" time="$time">
        <% if ((failure ?: '') != '') { %>
        <failure message="$failure" type="failed"><![CDATA[$failureDetails]]></failure>
        <% } %>
        <% if ((error ?: '') != '') { %>
        <error message="$errorDetails" type="$error"><![CDATA[$errorDetails]]></error>
        <% } %>
    </testcase>
    <system-out><![CDATA[$systemOut]]></system-out>
    <system-err><![CDATA[$systemErr]]></system-err>
</testsuite>